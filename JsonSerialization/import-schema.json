{
  "$id": "http://declarator.org/static/api/import-schema.json",
  "type": "object",
  "properties": {
    "person": {
      "$id": "/properties/person",
      "type": "object",
      "properties": {
        "name": {
          "$id": "/properties/person/properties/name",
          "type": "string",
          "title": "Фамилия Имя Отчество, указанные в документе",
          "default": "",
          "examples": [
            "Бродский Михаил Наумович"
          ]
        },
        "family_name": {
          "$id": "/properties/person/properties/family_name",
          "type": "string",
          "title": "Фамилия",
          "description": "Заполняется, если было в отдельной ячейке/столбце в исходном документе",
          "default": "",
          "examples": [
            "Бродский"
          ]
        },
        "given_name": {
          "$id": "/properties/person/properties/given_name",
          "type": "string",
          "title": "Имя",
          "description": "Заполняется, если было в отдельной ячейке/столбце в исходном документе",
          "default": "",
          "examples": [
            "Михаил"
          ]
        },
        "patronymic_name": {
          "$id": "/properties/person/properties/patronymic_name",
          "type": "string",
          "description": "Заполняется, если было в отдельной ячейке/столбце в исходном документе",
          "title": "Отчество",
          "default": "",
          "examples": [
            "Наумович"
          ]
        },
        "initials": {
          "$id": "/properties/person/properties/initials",
          "type": "string",
          "description": "Заполняется, если были в отдельной ячейке/столбце в исходном документе",
          "title": "Инициалы",
          "default": "",
          "examples": [
            "М.Н."
          ]
        },
        "role": {
          "$id": "/properties/person/properties/role",
          "type": "string",
          "title": "Должность",
          "default": "",
          "examples": [
            "Представитель Губернатора Санкт-Петербурга в Законодательном Собрании Санкт-Петербурга"
          ]
        }
      },
      "required": [
        "name"
      ]
    },
    "office": {
      "$id": "/properties/office",
      "type": "object",
      "properties": {
        "name": {
          "$id": "/properties/office/properties/name",
          "type": "string",
          "title": "Название организации, составившей документ",
          "default": "",
          "examples": [
            "Правительство Санкт-Петербурга"
          ]
        },
        "url": {
          "$id": "/properties/office/properties/url",
          "type": "string",
          "title": "URL сайта организации",
          "default": "",
          "examples": [
            "http://gov.spb.ru/"
          ]
        },
        "type": {
          "$id": "/properties/office/properties/type",
          "type": "string",
          "title": "Тип организации",
          "default": "",
          "examples": [
            "Региональный со структурой"
          ]
        },
        "region": {
          "$id": "/properties/office/properties/region",
          "type": "string",
          "title": "Регион",
          "default": "",
          "examples": [
            "Санкт-Петербург"
          ]
        }
      },
      "required": [
        "name"
      ]
    },
    "party": {
      "$id": "/properties/party",
      "type": "string",
      "title": "Партия, к которой пренадлежит персона",
      "default": "",
      "examples": [
        "Единая Россия"
      ]
    },
    "year": {
      "$id": "/properties/year",
      "type": "integer",
      "title": "Год декларируемых доходов",
      "default": 0,
      "examples": [
        2016
      ]
    },
    "document": {
      "$id": "/properties/document",
      "type": "object",
      "properties": {
        "type": {
          "$id": "/properties/document/properties/type",
          "type": "string",
          "title": "The Type Schema ",
          "default": "",
          "examples": [
            "Антикоррупционная декларация"
          ]
        },
        "name": {
          "$id": "/properties/document/properties/name",
          "type": "string",
          "title": "Название документа",
          "default": "",
          "examples": [
            "Сведения о доходах, об имуществе и обязательствах имущественного характера Губернатора Санкт-Петербурга, а также его супруги за период с 1 января 2016 года по 31 декабря 2016 года"
          ]
        },
        "url": {
          "$id": "/properties/document/properties/url",
          "type": "string",
          "title": "The Url Schema ",
          "default": "",
          "examples": [
            "http://gov.spb.ru/static/writable/mediact/materials/2017/05/22/%D0%A1%D0%B2%D0%B5%D0%B4%D0%B5%D0%BD%D0%B8%D1%8F_%D0%BE_%D0%B4%D0%BE%D1%85%D0%BE%D0%B4%D0%B0%D1%85_-_%D0%93%D1%83%D0%B1%D0%B5%D1%80%D0%BD%D0%B0%D1%82%D0%BE%D1%80_%D0%A1%D0%9F%D0%B1_-_2016.doc"
          ]
        },
        "page_url": {
          "$id": "/properties/document/properties/page_url",
          "type": "string",
          "title": "The Page_url Schema ",
          "default": "",
          "examples": [
            "http://gov.spb.ru/gov/admin/incomes/"
          ]
        }
      }
    },
    "incomes": {
      "$id": "/properties/incomes",
      "type": "array",
      "items": {
        "$id": "/properties/incomes/items",
        "type": "object",
        "properties": {
          "size": {
            "$id": "/properties/incomes/items/properties/size",
            "type": "integer",
            "title": "The Size Schema ",
            "default": 0,
            "examples": [
              1474895
            ]
          },
          "relative": {
            "$id": "/properties/incomes/items/properties/relative",
            "type": [ "string", "null" ],
            "title": "Родственник",
            "default": null,
            "enum": [
                null, 
                "Супруг(а)", 
                "Сын",
                "Дочь",
                "Иное",
                "Ребенок"
            ],
            "examples": [
              null
            ]
          }
        },
        "required": [
          "size",
          "relative"
        ]
      }
    },
    "real_estates": {
      "$id": "/properties/real_estates",
      "type": "array",
      "items": {
        "$id": "/properties/real_estates/items",
        "type": "object",
        "properties": {
          "type": {
            "$id": "/properties/real_estates/items/properties/type",
            "type": "string",
            "title": "The Type Schema ",
            "default": "",
            "examples": [
              "Земельный участок"
            ]
          },
          "square": {
            "$id": "/properties/real_estates/items/properties/square",
            "type": "integer",
            "title": "The Square Schema ",
            "default": 0,
            "examples": [
              1555
            ]
          },
          "country": {
            "$id": "/properties/real_estates/items/properties/country",
            "type": "string",
            "title": "The Country Schema ",
            "default": "",
            "examples": [
              "Россия"
            ]
          },
          "region": {
            "$id": "/properties/real_estates/items/properties/region",
            "type": "string",
            "title": "The Region Schema ",
            "default": "",
            "examples": [
              "Санкт-Петербург"
            ]
          },
          "own_type": {
            "$id": "/properties/real_estates/items/properties/own_type",
            "type": "string",
            "title": "The Own_type Schema ",
            "default": "",
            "examples": [
              "В собственности"
            ]
          },
          "share_type": {
            "$id": "/properties/real_estates/items/properties/share_type",
            "type": "string",
            "title": "The Share_type Schema ",
            "default": "",
            "examples": [
              "Долевая собственность"
            ]
          },
          "share_amount": {
            "$id": "/properties/real_estates/items/properties/share_amount",
            "type": [ "number", "null" ],
            "title": "The Share_amount Schema ",
            "default": 0,
            "examples": [
              0.5
            ]
          },
          "relative": {
            "$id": "/properties/real_estates/items/properties/relative",
            "type": "null",
            "title": "Родственник",
            "default": null,
            "enum": [
                null, 
                "Супруг(а)", 
                "Сын",
                "Дочь",
                "Иное",
                "Ребенок"
            ],
            "examples": [
              null
            ]
          }
        },
        "required": [
          "type",
          "square",
          "country",
          "own_type",
          "share_type",
          "share_amount"
        ]
      }
    },
    "vehicles": {
      "$id": "/properties/vehicles",
      "type": "array",
      "items": {
        "$id": "/properties/vehicles/items",
        "type": "object",
        "properties": {
          "text": {
            "$id": "/properties/vehicles/items/properties/text",
            "type": "string",
            "title": "The Text Schema ",
            "default": "",
            "examples": [
              "а/м легковой Лексус RX350"
            ]
          },
          "type": {
            "$id": "/properties/vehicles/items/properties/type",
            "type": "string",
            "title": "The Type Schema ",
            "default": "",
            "examples": [
              "Автомобиль легковой"
            ]
          },
          "brand": {
            "$id": "/properties/vehicles/items/properties/brand",
            "type": "string",
            "title": "The Brand Schema ",
            "default": "",
            "examples": [
              "Lexus"
            ]
          },
          "model": {
            "$id": "/properties/vehicles/items/properties/model",
            "type": "string",
            "title": "The Model Schema ",
            "default": "",
            "examples": [
              "RX350"
            ]
          },
          "manufacture_year": {
            "$id": "/properties/vehicles/items/properties/manufacture_year",
            "type": "null",
            "title": "The Manufacture_year Schema ",
            "default": null,
            "examples": [
              null
            ]
          },
          "relative": {
            "$id": "/properties/vehicles/items/properties/relative",
            "type": "null",
            "title": "Родственник",
            "default": null,
            "enum": [
                null, 
                "Супруг(а)", 
                "Сын",
                "Дочь",
                "Иное",
                "Ребенок"
            ],

            "examples": [
              null
            ]
          }
        },
        "required": [
          "text"
        ]
      }
    },
    "spendings": {
      "$id": "/properties/spendings",
      "type": "array",
      "items": {
        "$id": "/properties/spendings/items",
        "type": "object",
        "properties": {
          "text": {
            "$id": "/properties/spendings/items/properties/text",
            "type": "string",
            "title": "Текст накоплений из документа декларации",
            "default": "",
            "examples": [
              "Квартира (накопления за предыдущие годы)"
            ]
          }
        },
        "required": [
          "text"
        ]
      }
    }
  },
  "required": [
    "person",
    "office",
    "year",
    "incomes"
  ]
}