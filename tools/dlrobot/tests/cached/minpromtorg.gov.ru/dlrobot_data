        <script>

            $(document).ready(function(){
                setTimeout(function() {
                    $('.tabcontent input[type="checkbox"]').styler();
                }, 100)

                $( ".oneline-check" ).on( "click", function() {
                    $(this).toggleClass('active');
                    if($(this).hasClass('active')) {
                        $(this).find('input[type="checkbox"]').attr('checked', 'checked').trigger('refresh');
                    } else {
                        $(this).find('input[type="checkbox"]').removeAttr('checked').trigger('refresh');
                    }
                });
            });
        </script>

