<script>
   function rebuildToResolution() {
       var $w = $(window).width();

       if($w <= 991 && $w >= 767) {
           $('[data-important_item="3"]').after($('[data-important_item="2"]'))
       }
       else {
           $('[data-important_item="2"]').after($('[data-important_item="3"]'))
       }
   }

   $(document).ready(function() {
       rebuildToResolution();
   });

   $( window ).resize(function() {
       rebuildToResolution();
   });

</script>
