            <span class='rbccontents_ee_block' id="rbccontents_ee_block.442811773.CONTENT_TEXT.37."></span>

<script >
    //@todo переработать

    function toggleSearchPanel(obj) {
        $('.bottomtabletblock').find('.oneblock').removeClass('active');
        $(obj).parent().addClass('active');
        $([document.documentElement, document.body]).animate({
            scrollTop: 0
        }, 1000);

    }

    function toggleImportant(obj) {
        if(location.pathname !== '/') {
            location.href = '/';
            return false;
        }
        var $popular = $('[data-sort_mode="popular"]');
        $('.bottomtabletblock').find('.oneblock').removeClass('active');
        $(obj).parent().addClass('active');
        $popular.click();
        $([document.documentElement, document.body]).animate({
            scrollTop: $popular.offset().top - 100
        }, 1000);

        return false;
    }

    function updateContainer() {
        if($(window).width() < 768) {
            $('#filtertopnews').hide();
            $('#mainbottom').css( { 'margin-top' : 0 } );
        }
        else {
            $('#filtertopnews').show();
            $('#mainbottom').attr('style', '');
        }
    }

    $(window).resize(function() {
        updateContainer();
    });

    $(document).ready(function() {
        $('.bottomtabletblock').find('.oneblock').removeClass('active');

        var paths = [
                { path: '/open_ministry/anti/', index: 2 },
                { path: '/press-centre/news/', index: 0 }
            ];

        $.each(paths, function(index, value) {
            if(location.pathname === value.path) {
                if(value.index === 0) {
                    var filter = getCookie('workSpace') ? JSON.parse(getCookie('workSpace')) : null;

                    if(location.href.indexOf('/press-centre/news/?popular') !== -1) {
                        if(filter) {
                            filter.sort_mode = 'popular';
                        }
                        $('.bottomtabletblock').find('.oneblock').eq(1).addClass('active');
                    }

                    if(location.href.indexOf('/press-centre/news/?all') !== -1) {
                        if(filter) {
                            filter.sort_mode = 'all';
                        }
                        $('.bottomtabletblock').find('.oneblock').eq(0).addClass('active');
                    }
                    if(filter) {
                        $.cookie('workSpace', JSON.stringify(filter), { expires: 14, path: '/' });
                    }
                }
                else {
                    $('.bottomtabletblock').find('.oneblock').eq(value.index).addClass('active');
                }

                return false;
            }
        });



        updateContainer();

        var $cacheActiveMobile = null;

        $(document).on("closeMobileSearch", function() {
            if($cacheActiveMobile) {
                $cacheActiveMobile.toggleClass('active');
            }
            $cacheActiveMobile = null;
            $('.scrolltop_div').removeClass('mob-search-active');
        });

        // ракрывашка для поиска
        $('.search_btn_js').on('click', function() {
            if(!$('.search_mob').hasClass('active')) {
                $cacheActiveMobile = $(this).parent().siblings().filter('.active');
            }
            $('.search_mob').toggleClass('active');
            $('.scrolltop_div').addClass('mob-search-active');
            $(this).parent().siblings().removeClass('active');
            $(this).parent().toggleClass('active');
            if(!$('.search_mob').hasClass('active')) {
                $(document).trigger("closeMobileSearch");
            }
        });
        $('.search-close').on('click', function() {
            $('.search_mob').removeClass('active');
            $('.bottomtabletblock').find('.oneblock').removeClass('active');
            $(document).trigger("closeMobileSearch");
        });

    });
</script>

