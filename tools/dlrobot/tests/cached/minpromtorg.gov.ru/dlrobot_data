<script>
    var favorites_link_html = '';
    var share_friend_link = '';
    if (favorites_link_html == '' || share_friend_link == '') {
        favorites_link_html = $("#favorites-link").html();
        share_friend_link = $("#share-friend-link").html();
        $("#favorites-link").parent().html('');
    }

    function mp_draw_favorites() {
        var soc_dest = $("#mb_wrap");
        if (soc_dest.length > 0) {
            if (/\/open_ministry\//.exec(window.location.toString())) {
                var $comment = $('.content-text-comment #comment-list').parent();
                if ($comment.length > 0 && soc_dest.has($comment).length == 0) {
                    soc_dest.parent().detach().insertBefore($comment);
                    soc_dest = $("#mb_wrap");
                }
            }
            if (typeof mpt_page == 'object' && typeof mpt_page.SHOW_SOC_BUTTONS !== 'undefined' && mpt_page.SHOW_SOC_BUTTONS && typeof m2_modal !== 'undefined' && typeof m2_modal.soc_like == 'function') {
                soc_dest.append('<div id="soc_but_bottom" class="page-bottom-social-panel"></div>');
                m2_modal.soc_like('soc_but_bottom', window.location.origin + window.location.pathname, document.title, '');
                soc_dest.addClass('has-page-bottom-social-panel');
            }
        }

        $('.favorite-share').data('href', '/?do_27=share_form');
        if (typeof (mpt_page) == 'undefined' || !mpt_page.HIDE_FAVORITE) {
            $("<div id=\"favorites-link\">" + favorites_link_html + "</div>").appendTo("#mb_wrap");
        }
        if (typeof (mpt_page) == 'undefined' || !mpt_page.HIDE_SHARE) {
            $("<div id=\"share-friend-link\">" + share_friend_link + "</div>").appendTo("#mb_wrap");
        }
        if ($('body .add-to-favorites').length > 0) {
            m_favorites.init_icons(window.location.origin + window.location.pathname, document.title, 'body');
        }
        //скрыть социальные иконки в низу страницы если они отключенны
        if (typeof mpt_page == 'object' && typeof mpt_page.HIDE_FAVORITES_LINKS && mpt_page.HIDE_FAVORITES_LINKS) {
            $("#favorites-link").hide();
            $("#share-friend-link").hide();
        }

        $(document).trigger('afterFavoritePanelDone');
    }

    if ($("#mb_wrap").length > 0) {
        mp_draw_favorites();
    } else {
        $(document).ready(function () {
            mp_draw_favorites();
        });
    }
</script>
