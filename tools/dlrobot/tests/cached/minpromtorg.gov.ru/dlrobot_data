    $(document).ready(function () {
        var slider3 = $('.slider-sidebar').bxSlider({
            slideWidth: 130,
            controls: false,
            minSlides: 2,
            maxSlides: 3,
            touchEnabled: false
        });

        $('a.pager-prev-sidebar').click(function () {
            var current = slider3.getCurrentSlide();
            slider3.goToPrevSlide(current) - 1;
        });
        $('a.pager-next-sidebar').click(function () {
            var current = slider3.getCurrentSlide();
            slider3.goToNextSlide(current) + 1;
        });
    });
