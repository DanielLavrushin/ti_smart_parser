<script>
  jQuery(document).ready(function ($) {
    $('.block__tab a').click(function (e) {
      e.preventDefault();
      var id = $(this).attr("href");
      $('.block__tab').removeClass('ui-state-active');
      $(this).parent().addClass('ui-state-active');
      $('.block__content').hide();
      $(id).show();
    });
  });
</script>
  
