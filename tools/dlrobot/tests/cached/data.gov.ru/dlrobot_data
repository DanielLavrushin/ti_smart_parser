    var currLang = jQuery('html').attr('lang');
    if (currLang == 'ru') {
      var languages = ['ru', 'en'];
    }
    else {
      languages = ['en', 'ru'];
    }

    var searchPage = '/opendata';

    var changeLanguage = function() {
      if (arguments.length > 0 && currLang != arguments[0]) {
        var url_parts =  window.location.href.split('?');
        window.location.href = url_parts[0] + '?language=' + arguments[0];
      }
    };

    _govWidget = {
      cssOrigin: '//gosbar.gosuslugi.ru',
      catalogOrigin: '//gosbar.gosuslugi.ru',
      cssTheme: 'gray',
      language: {
        visible: languages,
        onChange: changeLanguage
      },
      picker: [
        { text: 'Поиск по наборам',
          onItemSelect: function() {
            searchPage = '/opendata';
            _govWidget.localSearch = searchPage + '?query=%query%';
          }
        },
        { text: 'Поиск по сайту',
          onItemSelect: function() {
            searchPage = '/search';
