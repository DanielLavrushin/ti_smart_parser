<script>
$(function() {

	$('ul.menu > li.expanded > a').click(function(event){
		event.preventDefault();
		submenu = $(this).parent().find('> ul.menu');
		if(submenu.css('display') == 'none') {
		 	submenu.slideDown('fast');
		} else {
		 	submenu.slideUp('fast');
		}
	});

	$('#navigator_toggle').click(function(event){
		if($('#navigator').css('display') == 'none') {
		 	$('#navigator').slideDown('fast');
		} else {
		 	$('#navigator').slideUp('fast');
		}
	});

});
</script>

